# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

def zdpods
    pod 'pop', '~> 1.0.9'
end

target 'ZDPopDemo' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for ZDPopDemo
  zdpods

  target 'ZDPopDemoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ZDPopDemoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
